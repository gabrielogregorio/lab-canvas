{
  "extends": [
    "@parcel/config-default"
  ],
  "transformers": {
    "*.{ts,tsx}": ["@parcel/transformer-typescript-tsc"],
    "*.{wav,mp3}": ["@parcel/transformer-raw"],
    "*.{jpg,png}": ["@parcel/transformer-image"]
  },
  "reporters": [
    "...",
    "parcel-reporter-static-files-copy"
  ]
}